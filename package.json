{
  "name": "chat_bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && npm run start",
    "start": "node dist/main/app.js",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/app.js",
    "build": "rimraf dist && tsc && node scripts/copyfiles.js",
    "up": "npm run build && docker-compose up ",
    "down": "docker-compose down",
    "restart": "npm run down && npm run up",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- -c jest-integration.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chmodr": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "debug": "^4.3.1",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "lint-staged": "^10.5.3",
    "ncp": "^2.0.0",
    "npm-check": "^5.9.2",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "chmodr": "^1.2.0",
    "file-type": "^16.2.0",
    "npm": "^6.14.7",
    "uuid": "^8.3.2",
    "venom-bot": "^2.2.3"
  }
}
